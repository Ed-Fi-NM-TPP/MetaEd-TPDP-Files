        <xs:element name="CourseStudentAcademicRecordFact" type="EXTENSION-CourseStudentAcademicRecordFact" />
        <xs:element name="CourseStudentAssessmentFact" type="EXTENSION-CourseStudentAssessmentFact" />
        <xs:element name="CourseStudentFact" type="EXTENSION-CourseStudentFact" />
